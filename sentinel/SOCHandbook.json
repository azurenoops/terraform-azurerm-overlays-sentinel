{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "workspaceName": {
            "type": "string"
        },
        "location": {
            "type": "string"
        }
    },
    "variables": {},
    "resources": [
        {
            "name": "SOCHandbook",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2021-04-01",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/Azure/Azure-Sentinel/master/Solutions/SOC%20Handbook/Package/mainTemplate.json",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "workspace": {
                        "value": "[parameters('workspaceName')]"
                    },
                    "location": {
                        "value": "[parameters('location')]"
                    },
                    "workspace-location": {
                        "value": "[parameters('location')]"
                    },
                    "workbook1-name": {
                        "value": "Analytics Efficiency"
                    },
                    "workbook2-name": {
                        "value": "AnomaliesVisulization"
                    },
                    "workbook3-name": {
                        "value": "AnomalyData"
                    },
                    "workbook4-name": {
                        "value": "Attack Surface Reduction Dashboard"
                    },
                    "workbook5-name": {
                        "value": "Microsoft Sentinel Cost"
                    },
                    "workbook6-name": {
                        "value": "Security Alerts"
                    },
                    "workbook7-name": {
                        "value": "Incident overview"
                    },
                    "workbook8-name": {
                        "value": "IntSights IOC Workbook"
                    },
                    "workbook9-name": {
                        "value": "Investigation Insights"
                    },
                    "workbook10-name": {
                        "value": "MITRE ATT&CK Workbook"
                    },
                    "workbook11-name": {
                        "value": "Security Operations Efficiency"
                    },
                    "workbook12-name": {
                        "value": "Security Status"
                    },
                    "workbook13-name": {
                        "value": "Microsoft Sentinel Central"
                    }
                }
            }
        }
    ],
    "outputs": {}
}

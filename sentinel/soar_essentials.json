{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "workspaceName": {
            "type": "string"
        },
        "location": {
            "type": "string"
        }
    },
    "variables": {},
    "resources": [
        {
            "name": "SOAREssentials",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2021-04-01",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/Azure/Azure-Sentinel/master/Solutions/SentinelSOARessentials/Package/mainTemplate.json",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "workspace": {
                        "value": "[parameters('workspaceName')]"
                    },
                    "location": {
                        "value": "[parameters('location')]"
                    },
                    "workspace-location": {
                        "value": "[parameters('location')]"
                    },
                    "workbook1-name": {
                        "value": "Automation health"
                    },
                    "workbook2-name": {
                        "value": "Incident overview"
                    },
                    "workbook3-name": {
                        "value": "Security Operations Efficiency"                        
                    },
                    "workbook4-name": {
                        "value": "Incident Tasks Workbook"
                    }
                }
            }
        }
    ],
    "outputs": {}
}
